{
  "name": "firebase-functions-api",
  "version": "1.0.0",
  "scripts": {
    "build": "babel index.es2017.js -o index.js",
    "build:watch": "babel -w index.es2017.js -o index.js",

    "pretest": "eslint index.es2017.js",
    "test": ":",

    "prestart": "npm run build:watch &",
    "start": "firebase serve --only functions",

    "prestart:experimental": "npm run build:watch &",
    "start:experimental": "firebase experimental:functions:shell",

    "predeploy": "npm run build",
    "deploy": "firebase deploy --only functions"
  },
  "dependencies": {
    "cors": "^2.8.4",
    "firebase-admin": "^5.4.3",
    "firebase-functions": "^0.7.2",
    "moment": "^2.19.1",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2017": "^6.24.1",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "firebase-tools": "^3.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:RomansBermans/firebase-functions-api.git"
  },
  "contributors": [
    {
      "name": "Romans Bermans",
      "email": "romans.bermans@gmail.com"
    }
  ]
}
